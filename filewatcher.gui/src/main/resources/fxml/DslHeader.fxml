<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.wenzhe.filewatcher.gui.controller.DslHeaderController">
   <children>
      <ComboBox fx:id="actionBox" minHeight="-Infinity" minWidth="-Infinity">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </ComboBox>
      <ComboBox fx:id="modeBox" minWidth="-Infinity" />
      <Label minHeight="-Infinity" minWidth="-Infinity" prefWidth="40.0" text="watch" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <TextField fx:id="pathField" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="250.0" HBox.hgrow="ALWAYS" />
      <Button fx:id="chooseButton" mnemonicParsing="false" onAction="#chooseDirectory" text="..." />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
